/* pages/participant/participant.wxss */

page {
  height: 100%;
  background: #f5f5f5;
}

.head {
  height: 40%;
  background: #dc3a2f;
  text-align: center;
}

.task {
  margin-top: 40rpx;
  margin-bottom: 40rpx;
  display: inline-block;
}

.head text {
  color: #eecc9c;
  font-size: 40rpx;
  font-family: "微软黑体";
}

.head button {
  width: 80%;
  background: #eecc9c;
  color: #dc3a2f;
  font-weight: 700;
}

.state {
  font-weight: 900;
  margin-bottom: 70rpx;
}

.state text {
  font-size: 60rpx;
}

.redpage {
  height: 5%;
  background: #f0f0f0;
  color: #b9b9b9;
}

.other_prople {
  height: 150rpx;
  border: 1px solid #f5f5f5;
  border-bottom-color: #b9b9b9;
}

.user_p{
  
}
.get_page {
}
.partner {
  border-bottom: 1px solid #fefefe;
  height: 150rpx;
  display: flex;
  justify-content: space-between;
}

.node {
  width: 200rpx;
  display: inline-block;
  color: #8d8d8d;
  margin-left: 25rpx;
  margin-right: 50rpx;
  line-height: 150rpx;
}

.headimg {
  margin-top: 25rpx;
  margin-left: 25rpx;
  border-radius: 50%;
  width: 100rpx;
  height: 100rpx;
}

.image {
  width: 120rpx;
  height: 120rpx;
  margin-top: 15rpx;
  border: 1px solid #c1c1c1;
}

.tasks {
  margin: 0px;
  padding: 0px;
  background-color: white;
}

.examine {
  margin-top: 25rpx;
  text-align: center;
  font-size: 35rpx;
  width: 150rpx;
  height: 100rpx;
  line-height: 90rpx;
  border: 2px solid #dc3a2f;
  color: #dc3a2f;
}

