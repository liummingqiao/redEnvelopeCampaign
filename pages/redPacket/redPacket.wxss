  font-size: 35rpx;
  color: #555555;
}
.modal_text view{
  padding: 30rpx;
  border-bottom: 2rpx solid #55555548;
}
.modal-btn{
   z-index: 10000;
  font-weight: 600;
  color: green;
}
.modal-btn_hover{
  background-color: #cad4ce;
}
