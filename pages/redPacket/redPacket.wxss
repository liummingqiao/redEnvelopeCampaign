.modal{
 
  width: 100vh;
  height: 100vh;
  position: absolute;
  top: 0;
  left: 0;
  background-color: #555555;
  opacity: 0.8;
}
.modal_text{
  text-align: center;
  width: 600rpx;
  position: absolute;
  top: 388rpx;
